<?xml version='1.0' encoding='utf-8'?>
<widget id="nl.mvtk.drainbattery" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="AutoHideSplashScreen" value="true" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="FadeSplashScreen" value="true" />
    <preference name="FadeSplashScreenDuration" value=".25" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="ShowSplashScreenSpinner" value="true" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="TopActivityIndicator" value="gray" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="iAd">
        <param name="ios-package" value="CDViAd" />
    </feature>
    <feature name="PowerManagement">
        <param name="ios-package" value="PowerManagement" />
    </feature>
    <feature name="Accelerometer">
        <param name="ios-package" value="CDVAccelerometer" />
    </feature>
    <feature name="Compass">
        <param name="ios-package" value="CDVCompass" />
    </feature>
    <feature name="Camera">
        <param name="ios-package" value="CDVCamera" />
    </feature>
    <feature name="Geolocation">
        <param name="ios-package" value="CDVLocation" />
    </feature>
    <feature name="Vibration">
        <param name="ios-package" value="CDVVibration" />
    </feature>
    <feature name="Battery">
        <param name="ios-package" value="CDVBattery" />
    </feature>
    <name>Drain Battery</name>
    <description>
        Drain your battery quick and easy
    </description>
    <author email="drainbattery@mvtk.nl" href="http://mvtk.nl">
        Marco van 't Klooster
    </author>
    <content src="index.html" />
    <gap:plugin name="com.rjfun.cordova.plugin.iad" version="0.1.0" />
    <gap:plugin name="com.simplec.plugins.powermanagement" version="0.4.0" />
    <gap:plugin name="org.apache.cordova.camera" version="0.2.4" />
    <gap:plugin name="org.apache.cordova.battery-status" version="0.2.7" />
    <gap:plugin name="org.apache.cordova.device-motion" version="0.2.6" />
    <gap:plugin name="org.apache.cordova.device-orientation" version="0.3.5" />
    <gap:plugin name="org.apache.cordova.geolocation" version="0.3.6" />
    <gap:plugin name="org.apache.cordova.file-transfer" version="0.3.0" />
    <access origin="*" />
</widget>
